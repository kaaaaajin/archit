  <%= form_with model: @post, url: posts_path , method: :get, local: true do |f| %>
    <%= f.select :keyword, [ [ "投稿が新しい順": '"updated_at desc'], 
                             [ "投稿が古い順": "updated_at asc" ],
                             [ "いいねが多い順": "like_count desc" ],
                           ] %>
    <%= f.submit %>
  <% end %>                
